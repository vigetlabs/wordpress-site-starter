{
  "name": "alert-banner",
  "title": "Alert Banner",
  "description": "Displays the Alert Banner Component.",
  "icon": "bell",
  "category": "components",
  "textdomain": "wp-starter",
  "keywords": [
    "notification",
    "alert",
    "banner",
    "information",
    "bar",
    "important",
    "dismiss"
  ],
  "attributes": {
    "block_id": {
      "type": "string",
      "default": ""
    },
    "align": {
      "type": "string",
      "default": "full"
    },
    "backgroundColor": {
      "type": "string",
      "default": "dark-black"
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "padding": {
            "top": "var:preset|spacing|10",
            "bottom": "var:preset|spacing|10",
            "left": "var:preset|spacing|20",
            "right": "var:preset|spacing|20"
          }
        }
      }
    }
  },
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "dismissible",
      "label": "Dismissible"
    }
  ],
  "supports": {
    "jsx": true,
    "mode": false,
    "alignWide": true,
    "anchor": true,
    "color": {
      "background": true
    },
    "layout": {
      "default": {
        "type": "flex",
        "justifyContent": "space-between",
        "flexWrap": "nowrap"
      },
      "allowOrientation": false,
      "allowCustomContentAndWideSize": false
    }
  },
  "acf": {
    "mode": "preview"
  }
}
