{
  "name": "video-embed",
  "title": "Video Embed",
  "description": "Displays the Video Embed Component.",
  "icon": "embed-video",
  "category": "components",
  "textdomain": "wp-starter",
  "keywords": ["video","embed","youtube","vimeo","url","play"],
  "allowedBlocks":[ "core/heading","core/paragraph","core/embed" ],
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    }
  },
  "supports": {
    "jsx": true,
    "mode": false,
    "alignWide": true,
    "color": {
      "background": true
    },
    "layout": {
      "default": {
        "type": "constrained",
        "justifyContent": "center"
      },
      "allowOrientation": false,
      "allowSwitching": false,
      "allowCustomContentAndWideSize": false
    }
  },
  "acf": {
    "mode": "preview"
  }
}
