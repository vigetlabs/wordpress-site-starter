{
  "name": "text-image",
  "title": "Text & Image",
  "description": "Displays the Text & Image Component.",
  "icon": "align-left",
  "category": "components",
  "textdomain": "wp-starter",
  "keywords": ["photo", "upload", "media", "picture", "image", "align", "left", "right", "content", "text"],
  "attributes": {
    "align": {
      "type": "string",
      "default": "wide"
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "padding": {
            "top": "var:preset|spacing|20",
            "bottom": "var:preset|spacing|20",
            "left": "var:preset|spacing|20",
            "right": "var:preset|spacing|20"
          }
        }
      }
    }
  },
  "supports": {
    "jsx": true,
    "mode": false,
    "alignWide": true,
    "color": {
      "background": true
    },
    "spacing": {
      "padding": ["top", "bottom"]
    }
  },
  "acf": {
    "mode": "preview"
  }
}
