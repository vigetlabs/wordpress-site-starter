{
  "name": "acf-blocks-toolkit",
  "version": "0.1.0",
  "description": "ACF Block support and button icons.",
  "author": "Viget",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "build": "wp-scripts build",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:js:src": "wp-scripts lint-js ./src",
    "lint:js:src:fix": "wp-scripts lint-js ./src --fix",
    "start": "wp-scripts start",
    "packages-update": "wp-scripts packages-update",
    "update-pot": "wp i18n make-pot . languages/acf-blocks-toolkit.pot --exclude=src",
    "format": "prettier \"./src/**/*.{css,ts,tsx,js}\" --check",
    "format:fix": "prettier \"./src/**/*.{css,ts,tsx,js}\" --write"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "@wordpress/icons": "^9.35.0",
    "@wordpress/scripts": "^26.15.0",
    "prettier": "^3.2.5",
    "webpack-remove-empty-scripts": "^0.8.4"
  }
}
