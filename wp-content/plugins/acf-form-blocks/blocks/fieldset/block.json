{
  "name": "fieldset",
  "title": "Fieldset",
  "description": "Form fieldset group",
  "icon": "welcome-widgets-menus",
  "category": "forms",
  "textdomain": "acf-form-blocks",
  "keywords": ["input", "contact", "feedback", "group", "set"],
  "ancestor": ["acf/form"],
  "supports": {
    "jsx": true,
    "mode": false,
    "alignWide": false,
    "anchor": true
  },
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "isCheckboxGroup": {
      "type": "boolean",
      "default": false
    }
  },
  "providesContext": {
    "acffb/fieldsetId": "blockId"
  },
  "usesContext": ["acffb/formId"],
  "styles": [
    { "name": "default", "label": "Default", "isDefault": true },
    { "name": "inline", "label": "Inline" },
    { "name": "columns", "label": "Columns" }
  ],
  "variations": [
    {
      "name": "checkbox-group",
      "icon": "forms",
      "title": "Checkbox Group",
      "scope": ["inserter", "transform"],
      "innerBlocks": [ ["acf/legend"], ["acf/checkbox"], ["acf/checkbox"], ["acf/checkbox"] ],
      "attributes": {
        "isCheckboxGroup": true
      },
      "isActive": ["isCheckboxGroup"]
    }
  ],
  "style": "file:./style.css",
  "acf": {
    "mode": "preview"
  }
}
